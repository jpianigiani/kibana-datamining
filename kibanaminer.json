    {

    }
    ,
    
"FieldTransforms" : 
    {
        "time": "value",
        "@timestamp": "value[0:19]",
        "_index": "self.split_string(value,'_index', [1],'-')",
        "host": "value",
        "ident": "value",
        "severity": "value.ljust(length)",
        "message":"json.dumps(value,indent=4)",
        "default": "value.ljust(length)"
    } ,
"FieldTransformsAttributes":
    {
        "split_string":{ 
            "Site": "([a-z,A-Z]{3}[0-9]{3})",
            "Lineup":".{6}([a-z,A-Z][0-9]{2})",
            "Vendor":".{9}([a-z,A-Z]{3})",
            "vnfname":".{12}([a-z,A-Z]{4})",
            "vnfindex":".{16}([0-9]{2})",
            "vnfcname":".{18}([a-z,A-Z]{5})",
            "vnfcindex":".{23}([0-9]{2})",
            "vnfname-vnfcname":".{12}([a-z,A-Z]{4}).{2}([a-z,A-Z]{5})",
            "_index":"fluentd.(.*)-.*",
            "host":"(.*).poc|ims.telekom.de",
            "@timestamp":"([0-9]{4})-([0-9]{2})-([0-9]{2}T)([0-9]{2}:[0-9]{2}:[0-9]{2})"

        },
        "message_parser":{
                    "vnfname":"([a-z,A-Z]{3}[0-9]{3}[a-z,A-Z][0-9]{2}[a-z,A-Z]{3}[a-z,A-Z]{4}[0-9]{2}[a-z,A-Z]{5}[0-9]{2})|([a-z,A-Z]{3}[0-9]{3}[a-z,A-Z]{4}[0-9][a-z,A-Z]{3}[0-9])",
                    "computename":"(overcloud[a-z,0-9]{3}[-]compdpdk[0-9]{2}hw[0-9][-][0-9][.][a-z,A-Z]{3}[0-9]{3})",
                    "controllername":"(overcloud[a-z,0-9]{3}[-]ctrl[-][0-9][.][a-z,A-Z]{3}[0-9]{3})",
                    "routername":"([a-z,A-Z]{3}[0-9]{3}[a-z]{4}[0-9]{5})",
                    "othername":"(overcloud[a-z,0-9]{3}[-][a-z]{2}[-][0-9][.][a-z,A-Z]{3}[0-9]{3})",
                    "ipv4address":"([0-9]{1,3}[.][0-9]{1,3}[.][0-9]{1,3}[.][0-9]{1,3})",
                    "date":"([0-9]{4}[-\/.][0-9]{2}[-\/.][0-9]{2,})",
                    "time":"([0-9]{2}[:][0-9]{2}[:][0-9]{2}[ ,][0-9]{2,})",
                    "vendor": "(jnp|msw|mav|hpe|nok|netcracker|rhel|junos|contrail)",
                    "uuid":"([a-z,0-9]{8}[-][a-z,0-9]{4}[-][a-z,0-9]{4}[-][a-z,0-9]{4}[-][a-z,0-9]{11})",
                    "method": "(get|post|put)",
                    "response":"(200 ok)",
                    "severity":"(info|warn|debug|error)",
                    "lineup":"(\\w{3,}-[a-z,A-Z]{3}[0-9]{3}[-][a-z,A-Z][0-9]{2})",
                    "RepoName":"([a-z,A-Z]{3}[0-9]{3}[-]\\w{3,}-[a-z,A-Z][0-9]{2})",

                    "error":"error[:= ]\\w{1,}"
                    

                        }
